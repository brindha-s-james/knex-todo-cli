#!/usr/bin/env node

const commands = require('./commands')

const userInputs = process.argv
const cmd = userInputs[2]
const userId = userInputs[3]

switch (cmd) {
  case 'list':
    commands.list()
    break

  case 'idsearch':
    commands.displayTodo(userId)
    break

  case 'done':
    commands.deleteToDo(userId)
    break

  default:
    console.log(`I don't understand that command: ${cmd}`)
}
